{
    "name": "titanomachy/blacklist",
    "type": "roundcube-plugin",
    "description": "Roundcube plugin to provide a generic access blacklist.",
    "keywords": ["mail","roundcube","security"],
    "homepage": "https://github.com/titanomachy/roundcube-blacklist",
    "version": "0.2.1",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Diana Soares",
            "email": "diana.soares@gmail.com",
            "homepage": "https://github.com/dsoares"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        },
        {
            "type": "github",
            "url": "https://github.com/titanomachy/roundcube-geolocation"
        }
    ],
    "require": {
        "php": ">=5.4",
        "roundcube/plugin-installer": "~0.3.1",
        "titanomachy/geolocation": "~0.1.3"
    },
    "scripts": {
        "lint": "php-cs-fixer -v fix --diff --diff-format udiff --dry-run",
        "fix": "php-cs-fixer fix"
    },
    "extra": {
        "roundcube": {
            "min-version": "1.6"
        }
    }
}
